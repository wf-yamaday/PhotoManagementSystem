<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-app_2_4.xsd"
	version="2.4">
<!-- Servlet -->
	<servlet>
		<servlet-name>AuthenticationConfirmationServlet</servlet-name>
		<servlet-class>servlet.AuthenticationConfirmationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AuthenticationConfirmationServlet</servlet-name>
		<url-pattern>/AuthenticationConfirmation</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetInformationBoardServlet</servlet-name>
		<servlet-class>servlet.GetInformationBoardServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetInformationBoardServlet</servlet-name>
		<url-pattern>/Information</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>servlet.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UploadServlet</servlet-name>
		<servlet-class>servlet.UploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadServlet</servlet-name>
		<url-pattern>/UploadServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ViewAlbumServlet</servlet-name>
		<servlet-class>servlet.ViewAlbumServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewAlbumServlet</servlet-name>
		<url-pattern>/ViewAlbumServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ViewAlbumListServlet</servlet-name>
		<servlet-class>servlet.ViewAlbumListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewAlbumListServlet</servlet-name>
		<url-pattern>/ViewAlbumListServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ViewPhotoServlet</servlet-name>
		<servlet-class>servlet.ViewPhotoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewPhotoServlet</servlet-name>
		<url-pattern>/ViewPhotoServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddUplaodServlet</servlet-name>
		<servlet-class>servlet.AddUplaodServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddUplaodServlet</servlet-name>
		<url-pattern>/AddUpload</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetCreatedAlbumServlet</servlet-name>
		<servlet-class>servlet.GetCreatedAlbumServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetCreatedAlbumServlet</servlet-name>
		<url-pattern>/NewUpload</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RemovePhotoServlet</servlet-name>
		<servlet-class>servlet.RemovePhotoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RemovePhotoServlet</servlet-name>
		<url-pattern>/RemovePhoto</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EditAlbumServlet</servlet-name>
		<servlet-class>servlet.EditAlbumServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditAlbumServlet</servlet-name>
		<url-pattern>/EditAlbum</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RemoveAlbumServlet</servlet-name>
		<servlet-class>servlet.RemoveAlbumServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RemoveAlbumServlet</servlet-name>
		<url-pattern>/RemoveAlbum</url-pattern>
	</servlet-mapping>
<!-- Filter -->
	<filter>
		<filter-name>LoginFilter</filter-name>
		<filter-class>filter.LoginFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<servlet-name>LoginServlet</servlet-name>
	</filter-mapping>
	<filter>
		<filter-name>UploadFilter</filter-name>
		<filter-class>filter.UploadFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>UploadFilter</filter-name>
		<servlet-name>UploadServlet</servlet-name>
		<servlet-name>EditAlbumServlet</servlet-name>
	</filter-mapping>
	<filter>
		<filter-name>SessionFilter</filter-name>
		<filter-class>filter.SessionFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>SessionFilter</filter-name>
		<servlet-name>GetInformationBoardServlet</servlet-name>
		<servlet-name>ViewAlbumServlet</servlet-name>
		<servlet-name>ViewAlbumListServlet</servlet-name>
		<servlet-name>ViewPhotoServlet</servlet-name>
		<servlet-name>UploadServlet</servlet-name>
		<servlet-name>AddUplaodServlet</servlet-name>
		<servlet-name>GetCreatedAlbumServlet</servlet-name>
		<servlet-name>RemovePhotoServlet</servlet-name>
		<servlet-name>EditAlbumServlet</servlet-name>
	</filter-mapping>
<!-- ERROR page -->
	<error-page>
		<error-code>403</error-code>
		<location>/jsp/error/403.jsp</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/jsp/error/404.jsp</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/jsp/error/500.jsp</location>
	</error-page>
<!-- Security -->
	<security-constraint>
	<display-name>Jsp Security</display-name>
	<web-resource-collection>
		<web-resource-name>Protected Area</web-resource-name>
		<url-pattern>*.jsp</url-pattern>
	</web-resource-collection>
		<auth-constraint></auth-constraint>
	</security-constraint>
	
</web-app>